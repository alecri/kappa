cit <- c("\\citep{orsini2011meta}", "\\citep{liu2009two}", "\\citep{rota2010random}",
"\\citep{orsini2011meta}", "\\citep{crippa2016dose}", "\\citep{liu2009two}",
"\\citep{greenland1992methods}", "\\citep{orsini2006generalized}",
"\\citep{crippa2014coffee}", "\\citep{crippa2014coffee}", "\\citep{crippa2014coffee}",
"\\cite{nilsson2012traditional}", "\\citep{larsson2011coffee}", "\\citep{di2014fish}",
"\\citep{orsini2006generalized}", "\\citep{larsson2015milk}", "\\citep{larsson2006milk}",
"\\citep{berlin1993meta}", "\\citep{crippa2016red}", "\\citep{crippa2016red}",
"\\citep{crippa2018one}")
cbind(Name = Item,
Description = paste(d[, "Title"], cit)) %>%
kable(format = "latex", booktabs = T, escape = FALSE,
caption = "Data sets available in the \\pkg{dosresmeta} $\\R$ package.")%>%
kable_styling(full_width = F) %>%
column_spec(2, width = "32em")
